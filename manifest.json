{
  "description": "Clickup Ext",
  "manifest_version": 2,
  "name": "clickup-ext",
  "version": "0.1.0",
  "icons": {
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "clickup-ext"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.clickup.com/*"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "<all_urls>"
  ],
  "applications": {
    "gecko": {
      "id": "clickup-ext@snca.net",
      "strict_min_version": "57.0"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
